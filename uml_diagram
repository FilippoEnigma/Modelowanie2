// UML Diagram
digraph {
	node [fillcolor=gray90 shape=record style=filled]
	edge [fontcolor=blue fontsize=10]
	Users [label="Users|UserID: integer\l\lImie: string\l\lNazwisko: string\l\lEmail: string\l\lHaslo: string\l\lRola: string\l"]
	Location [label="Location|LocationID: integer\l\lMiasto: string\l\lKraj: string\l\lAdres: string\l\lPojemnosc: integer\l\lRodzajMiejsc: string\l"]
	Events [label="Events|EventID: integer\l\lNazwaWydarzenia: string\l\lData: date\l\lOpis: text\l\lLimitMiejsc: integer\l"]
	Events -> Location [label="MiejsceID (1..1)" arrowhead=vee]
	EventCategories [label="EventCategories|CategoryID: integer\l\lNazwaKategorii: string\l"]
	EventCategoryAssignment [label="EventCategoryAssignment|AssignmentID: integer\l"]
	EventCategoryAssignment -> Events [label="EventID (1..n)" arrowhead=diamond]
	EventCategoryAssignment -> EventCategories [label="CategoryID (1..n)" arrowhead=diamond]
	Discounts [label="Discounts|DiscountID: integer\l\lNazwaZnizki: string\l\lKwotaZnizki: decimal\l\lProcentZnizki: decimal\l"]
	Tickets [label="Tickets|TicketID: integer\l\lDataZakupu: datetime\l\lStatus: string\l"]
	Tickets -> Users [label="UserID (1..1)" arrowhead=vee]
	Tickets -> Events [label="EventID (1..1)" arrowhead=vee]
	Tickets -> Discounts [label="ZnizkaID (0..1)" arrowhead=vee]
	Payments [label="Payments|PaymentID: integer\l\lKwota: decimal\l\lDataPlatnosci: datetime\l\lMetodaPlatnosci: string\l"]
	Payments -> Users [label="UserID (1..n)" arrowhead=diamond]
	EventRatings [label="EventRatings|RatingID: integer\l\lOcena: decimal\l\lKomentarz: text\l"]
	EventRatings -> Events [label="EventID (1..n)" arrowhead=diamond]
	EventRatings -> Users [label="UserID (1..n)" arrowhead=diamond]
	Administrators [label="Administrators|AdminID: integer\l"]
	Administrators -> Users [label="UserID (1..1)" arrowhead=vee]
}
